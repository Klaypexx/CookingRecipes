meta {
  name: [Teardown][3] Logout
  type: http
  seq: 3
}

post {
  url: {{baseUrl}}/auth/logout
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 200
}

script:post-response {
  let responseBody = res.getBody();
  
  if (res.getStatus() == 200) {
    bru.setEnvVar("token", "")
  }
}
