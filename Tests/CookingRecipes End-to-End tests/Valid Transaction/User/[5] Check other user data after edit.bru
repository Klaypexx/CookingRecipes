  type: http
  seq: 5
}

get {
  url: {{baseUrl}}/users
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

tests {
  test("status code 200", function() {
    const data = res.getBody();
    expect(res.getStatus()).to.equal(200);
  });
  
  test("name value is Edit Test", function() {
    const data = res.getBody();
    expect(data.name).to.be.a("string");
    expect(data.name).to.equal("Edit Test");
  });
  
  test("username value is Edit Test", function() {
    const data = res.getBody();
    expect(data.userName).to.be.a("string");
    expect(data.userName).to.equal("Edit Test");
  });
  
  test("description is Edit Test", function() {
    const data = res.getBody();
    expect(data.description).to.be.a("string");
    expect(data.description).to.equal("Edit Test");
  });
}
