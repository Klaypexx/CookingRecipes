meta {
  name: [2] Get user info
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/users
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

tests {
  test("status code 200", function() {
    const data = res.getBody();
    expect(res.getStatus()).to.equal(200);
  });
  
  test("name value is test", function() {
    const data = res.getBody();
    expect(data.name).to.be.a("string");
    expect(data.name).to.equal("test");
  });
  
  test("username value is test", function() {
    const data = res.getBody();
    expect(data.userName).to.be.a("string");
    expect(data.userName).to.equal("test");
  });
  
  test("description is undefined", function() {
    const data = res.getBody();
    expect(data.description).to.be.a("string");
    expect(data.description).to.equal("");
  });
}
